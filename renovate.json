{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "extends": [
    "config:recommended"
  ],
  "packageRules": [
    {
      "matchManagers": [
        "dockerfile"
      ],
      "matchPackageNames": [
        "lscr.io/linuxserver/qbittorrent"
      ],
      "extractVersion": "(\\d+\\.\\d+\\.\\d+)(?:-ls\\d+)?",
      "allowedVersions": "^4.0.0",
      "versioning": "semver"
    }
  ]
}
